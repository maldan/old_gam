{
  "name": "@maldan/gam",
  "version": "1.0.0",
  "description": "Git Application Manager",
  "main": "bin/index.js",
  "scripts": {
    "run": "node --unhandled-rejections=strict ./bin/index.js",
    "dev": "tsc && node --unhandled-rejections=strict ./bin/index.js",
    "build": "tsc",
    "prepublish": "tsc",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "coverage": "nyc --temp-dir bin/nyc --report-dir bin/public/coverage -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "keywords": [
    "application manager",
    "git"
  ],
  "author": "Maldan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maldan/gam.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.22",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "yargs": "^16.2.0"
  }
}
